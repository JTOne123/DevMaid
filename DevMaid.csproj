<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <!-- <RuntimeIdentifier>win10-x64</RuntimeIdentifier> -->
    <Description>DevTools for help you.</Description>
    <LangVersion>Latest</LangVersion>
    <VersionPrefix>0.0.5</VersionPrefix>
    <Authors>Filiphe Vilar Figueiredo</Authors>
    <Product>DevMaid</Product>
    <Copyright>GNU General Public License v3.0</Copyright>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://github.com/liphvf/DevMaid/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/liphvf/DevMaid</PackageProjectUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <DebugType>embedded</DebugType>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <IsPackable>true</IsPackable>
    <PackAsTool>true</PackAsTool>
    <TieredCompilation>true</TieredCompilation>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageTags>Tools</PackageTags>
    <RepositoryUrl>https://github.com/liphvf/DevMaid</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageIconUrl>https://raw.githubusercontent.com/liphvf/DevMaid/master/icon.png</PackageIconUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.CommandLineUtils" Version="1.1.1" />
    <PackageReference Include="System.Text.Encoding.CodePages" Version="4.5.0" />
    <PackageReference Include="Npgsql" Version="4.0.3" />
  </ItemGroup>
</Project>